<style>
    img{
        width:150px;
    }
</style>
<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Address</th>
            <th scope="col">Image</th>
            <th scope="col">Details</th>
      
        </tr>
    </thead>
    <tbody>
        @foreach(Student item in Model)
        {<tr>
            <th scope="row">@item.Id</th>
            <td>@item.Name</td>
            <td>@item.Address</td>
            <td><img src="/Images/@item.Image"/></td>
                <td><a href="GetSTBYTD/@item.Id">Details</a></td>
        </tr>
        }        
    </tbody>
</table>